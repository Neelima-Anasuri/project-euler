# How many composite integers, n < 10^8, have precisely two, not necessarily distinct, prime factors?

# https://projecteuler.net/problem=187

require('ntheory')

var limit = 10**8
var primes = %S<ntheory>.primes(0, limit/2)

var count = 0
var end = primes.end

for i in ^primes {
    for j in (i .. end) {
        primes[i]*primes[j] >= limit ? break : ++count
    }
}

say count
